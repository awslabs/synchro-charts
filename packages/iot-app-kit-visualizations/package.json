{
  "name": "@iot-app-kit/charts-core",
  "description": "IoT App Kit Visualizations",
  "version": "2.1.1",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com/"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/interface.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/iot-app-kit-visualizations/iot-app-kit-visualizations.js",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/synchro-charts.git"
  },
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "clean": "rm -rf dist && rm -rf screenshot",
    "test": "npm-run-all -p test:unit-test test:typescript",
    "test:dev": "npm-run-all test test:integ",
    "test:unit-test": "TZ=UTC stencil test --spec --coverage",
    "test:cypress": "cypress run",
    "test:cypress-dev": "cypress open --env disableSnapshotTests=true",
    "test:typescript": "tsc --noEmit",
    "test:watch": "TZ=UTC stencil test --spec --watchAll",
    "lint": "npm-run-all -p lint:eslint lint:stylelint",
    "fix": "yarn lint:fix",
    "lint:fix": "npm-run-all -p lint:fix-eslint lint:fix-stylelint",
    "lint:fix-eslint": "eslint --fix --ext .js,.ts,.tsx ./",
    "lint:fix-stylelint": "stylelint 'src/**/*.css' --fix",
    "lint:eslint": "eslint --ext .js,.ts,.tsx ./ --max-warnings 0",
    "lint:stylelint": "stylelint 'src/**/*.css' --max-warnings 0",
    "release": "yarn clean && npm-run-all -p build test lint",
    "copy:license": "cp ../../LICENSE LICENSE",
    "copy:notice": "cp ../../NOTICE NOTICE",
    "copy:code-of-conduct": "cp ../../CODE_OF_CONDUCT.md CODE_OF_CONDUCT.md",
    "copy:contributing": "cp ../../CONTRIBUTING.md CONTRIBUTING.md",
    "prepack": "yarn run copy:license && yarn run copy:notice && yarn run copy:code-of-conduct && yarn run copy:contributing",
    "prepublishOnly": "yarn release",
    "pack": "yarn pack"
  },
